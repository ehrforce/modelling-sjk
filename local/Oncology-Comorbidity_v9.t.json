  }, {
    "@type" : "TEMPLATE_OVERLAY",
    "uid" : "efec5af9-4958-4dea-9249-335660962230",
    "description" : {
      "@type" : "RESOURCE_DESCRIPTION",
      "originalAuthor" : { },
